import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
        int arr[] = new int [n];
        for(int i = 0;i<n;i++)arr[i]= in.nextInt();
        for(int i = 0 ;i<n/2;i++){
            System.out.println( (arr[i]+arr[n-i-1])/10 +" "+ (arr[i]+arr[n-i-1])%10);
        }
	}
}
